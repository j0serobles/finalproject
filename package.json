{
  "name": "finalproject",
  "version": "1.0.0",
  "description": "UCF COding Bootcamp Final Project",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod" : "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "test"  : "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "npm run build"
  },
  "author": "engjoserobles@gmail.com,mjnorato@aol.com",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}